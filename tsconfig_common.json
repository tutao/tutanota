{
  "compilerOptions": {
	"lib": [
	  "ES2020",
	  "webworker",
	  "dom"
	],
	"allowJs": true,
	"checkJs": false,
	"target": "ES2020",
	"module": "ES2020",
	"moduleResolution": "node",
	"skipLibCheck": true,
	"declaration": true,
	"noEmitOnError": true,
	"noErrorTruncation": true,
	"allowSyntheticDefaultImports": true,
	"noImplicitAny": false,
	"noImplicitThis": true,
	"noUnusedLocals": false,
	"noUnusedParameters": false,
	"esModuleInterop": true,
	"strict": false,
	"paths": {
	  "global": [
		"./types/globals.d.ts"
	  ]
	}
  }
}
