{
	"name": "tutanota",
	"version": "3.113.2",
	"license": "GPL-3.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/tutao/tutanota.git"
	},
	"type": "module",
	"exports": {
		"./*": "./build/prebuilt/*",
		"./buildSrc/*": "./buildSrc/*"
	},
	"scripts": {
		"build-packages": "node buildSrc/buildPackages.js all",
		"build-runtime-packages": "node buildSrc/buildPackages.js runtime",
		"start": "./start-desktop.sh",
		"test": "npm run --if-present test -ws && cd test && node test",
		"test:app": "cd test && node test",
		"fasttest": "cd test && node test -f",
		"types": "tsc --incremental true --noEmit true",
		"prebuild": "node buildSrc/prebuild.js",
		"preinstall": "git config core.hooksPath githooks || true",
		"postinstall": "node buildSrc/postinstall.js",
		"bump-version": "node buildSrc/bump-version.js",
		"generate-ipc": "npm run build -w @tutao/licc && licc ./ipc-schema",
		"style:check": "prettier -c \"**/*.(ts|js|json|json5)\"",
		"style:fix": "prettier -w \"**/*.(ts|js|json|json5)\"",
		"lint:check": "eslint .",
		"lint:fix": "eslint --fix .",
		"check": "npm run style:check && npm run lint:check",
		"fix": "npm run style:fix && npm run lint:fix"
	},
	"dependencies": {
		"@tutao/oxmsg": "0.0.9-beta.0",
		"@tutao/tutanota-crypto": "3.113.2",
		"@tutao/tutanota-usagetests": "3.113.2",
		"@tutao/tutanota-utils": "3.113.2",
		"@types/better-sqlite3": "7.4.2",
		"@types/dompurify": "2.4.0",
		"@types/linkifyjs": "2.1.4",
		"@types/luxon": "3.2.0",
		"@types/mithril": "2.0.11",
		"@types/qrcode-svg": "1.1.1",
		"@types/systemjs": "6.1.1",
		"@types/winreg": "1.2.31",
		"better-sqlite3": "git+https://github.com/tutao/better-sqlite3-sqlcipher#e2c61e6122bc56c6cfc29e61d21001faf43e2b8e",
		"cborg": "1.5.4",
		"dompurify": "2.4.3",
		"electron": "24.2.0",
		"electron-updater": "6.1.0",
		"jszip": "3.10.1",
		"keytar": "git+https://github.com/tutao/node-keytar#12593c5809c9ed6bfc063ed3e862dd85a1506aca",
		"linkifyjs": "3.0.5",
		"luxon": "3.2.1",
		"mithril": "2.2.2",
		"qrcode-svg": "1.0.0",
		"squire-rte": "2.0.3",
		"systemjs": "6.10.2",
		"winreg": "1.2.4"
	},
	"devDependencies": {
		"@electron/notarize": "1.2.3",
		"@octokit/auth-token": "2.5.0",
		"@octokit/rest": "18.12.0",
		"@rollup/plugin-commonjs": "24.0.1",
		"@rollup/plugin-json": "6.0.0",
		"@rollup/plugin-node-resolve": "15.0.1",
		"@rollup/plugin-terser": "0.4.0",
		"@rollup/plugin-typescript": "11.0.0",
		"@tutao/licc": "3.113.2",
		"@tutao/tutanota-test-utils": "3.113.2",
		"@typescript-eslint/eslint-plugin": "5.15.0",
		"body-parser": "1.20.0",
		"chokidar": "3.5.2",
		"commander": "9.2.0",
		"electron-builder": "24.2.1",
		"electron-packager": "17.1.1",
		"esbuild": "0.17.15",
		"esbuild-plugin-alias-path": "2.0.2",
		"eslint": "8.38.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-unicorn": "46.0.0",
		"express": "4.18.2",
		"fs-extra": "10.0.0",
		"full-icu": "1.3.4",
		"js-yaml": "4.1.0",
		"jsdom": "20.0.0",
		"node-gyp": "9.0.0",
		"octokit": "1.3.0",
		"ospec": "https://github.com/tutao/ospec.git#0472107629ede33be4c4d19e89f237a6d7b0cb11",
		"prettier": "2.8.1",
		"rollup": "3.20.2",
		"testdouble": "3.16.4",
		"typescript": "5.0.3",
		"xhr2": "0.2.1",
		"zx": "7.0.7"
	},
	"workspaces": [
		"./packages/*"
	],
	"engines": {
		"npm": ">=8.0.0"
	}
}
