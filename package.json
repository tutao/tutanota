	"@rollup/plugin-node-resolve": "13.1.3",
	"@rollup/plugin-typescript": "8.3.0",
	"@tutao/tutanota-build-server": "3.95.4",
	"@tutao/tutanota-test-utils": "3.95.4",
	"body-parser": "1.19.0",
	"chokidar": "3.5.2",
