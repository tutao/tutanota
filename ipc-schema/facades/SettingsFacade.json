			"arg": [{ "name": "DesktopConfigKey" }],
			"ret": "string?"
		},
		"setStringConfigValue": {
			"arg": [{ "name": "DesktopConfigKey" }, { "value": "string?" }],
			"ret": "void"
		},
		"getBooleanConfigValue": {
			"arg": [{ "name": "DesktopConfigKey" }],
			"ret": "boolean"
		},
