			"arg": [],
			"ret": "List<string>"
		},
		"getIntegrationInfo": {
			"arg": [],
			"ret": "IntegrationInfo"
		},
		"enableAutoLaunch": {
			"arg": [],
			"ret": "void"
		},
		"disableAutoLaunch": {
			"arg": [],
			"ret": "void"
		},
		"manualUpdate": {
			"arg": [],
			"ret": "boolean"
		},
